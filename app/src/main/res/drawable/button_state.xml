<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Состояние по умолчанию (контур и голубой текст) -->
    <item android:drawable="@drawable/button_border" android:state_enabled="true"/>

    <!-- Состояние при нажатии (заливка голубым и белый текст) -->
    <item android:drawable="@drawable/button_pressed" android:state_pressed="true"/>
</selector>
